#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n;
scanf("%d", &n);
if (n < 1 || n > 10000000) {
    printf("INVALID INPUT\n");
    return 1;
}
if (n == 1 || n == 2) {
    printf("1\n");
    return 0;
}
unsigned long long a = 1, b = 1, c;
for (int i = 3; i <= n; i++) {
    c = a + b;
    a = b;
    b = c;
}
printf("%llu\n", c);
    return 0;
}
