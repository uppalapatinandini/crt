#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,w;
    float h;
    scanf("%d %d %f",&a,&w,&h);
    if(a>18){
        if(w>45){
            if(h>5.5){
                printf("He or She can donate blood");
            }
        }
    }else{
        printf("He or She unable to donate blood");
    }   
    return 0;
}
