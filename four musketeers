#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

   double x1, y1, x2, y2, x3, y3;
    scanf("%lf %lf %lf %lf %lf %lf", &x1, &y1, &x2, &y2, &x3, &y3); 
    double c_x = (x1 + x2 + x3) / 3.0;
    double c_y = (y1 + y2 + y3) / 3.0; 
    printf("%.1f\n%.1f\n", c_x, c_y);
    return 0;
}
